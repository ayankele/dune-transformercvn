{
  "hidden_dim": 128,

  "initial_feature_dim": 8,
  "initial_pixel_dim": 64,
  "final_decoder_dim": 16,

  "feature_embedding_dim":  32,
  "pixel_embedding_dim":  256,
  "position_embedding_dim": 32,

  "num_encoder_layers": 6,
  "num_prong_decoder_layers": 4,

  "num_attention_heads": 8,
  "transformer_activation": "gelu",
  "linear_prelu_activation": true,
  "linear_batch_norm": true,

  "disable_smart_features": true,
  "normalize_features": true,
  "one_hot_pixels": false,
  "log_pixels": false,

  "densenet_structure": [3, 6, 12, 6, 3],
  "densenet_growth_rate": 32,
  "densenet_batch_norm_size": 4,

  "pixel_noise_std": 0.001,
  "dropout": 0.1,

  "epochs": 1024,
  "batch_size": 16,
  "optimizer": "AdamW",
  "event_prong_loss_proportion": 0.9,
  "event_current_targets": true,

  "learning_rate": 0.0000075665331738495,
  "learning_rate_cycles": 16,
  "learning_rate_warmup_epochs": 1.0,
  "l2_penalty": 2.1300208077466406e-05,
  "gradient_clip": 43,
  "loss_gamma": 1.0,

  "training_file": "./data/dune/training_prong_pixels_minkowski_sparse.h5",
  "train_validation_split": 0.95,
  "num_dataloader_workers": 16,
  "dataset_limit": 1.0,
  "num_gpu": 4,
  "verbose_output": 1
}